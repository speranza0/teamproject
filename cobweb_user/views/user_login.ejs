<body class="register-page">
    <div class="register-container">
        <div class="container">
            <div class="register-card">
                <a class="logo" href="index">
                    <img src="../img/LOGO2.png" class="logo"></a>
                <h6 class="text-center"><strong>Login</strong></h6>
                <form method="POST" action="/login">
                    <div class="form-group">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fas fa-envelope"></i></span>
                            <input class="form-control" type="email" name="user_id" placeholder="Email">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fas fa-key"></i></span>
                            <input class="form-control" type="password" name="user_pwd" placeholder="Password">
                        </div>
                    </div>
                    <button type="submit" class="btn btn-primary">Login</button>
                    <p class="note"><a href="forgot-password">Forgot Password?</a>
                        <span class="pull-right">Don't have an account? <a href="/register">Register</a>
                        </span></p>
                </form>
            </div>
